[editor_plugins]

enabled=PackedStringArray("res://addons/AsepriteWizard/plugin.cfg", "res://addons/inkgd/plugin.cfg")

[gui]

theme/custom="res://src/dinky_theme.tres"

[inkgd]

register_templates=false

